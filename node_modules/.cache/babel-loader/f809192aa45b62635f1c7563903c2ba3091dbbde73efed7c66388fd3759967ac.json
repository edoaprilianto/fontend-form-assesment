{"ast":null,"code":"import axios from 'axios';\nimport LayoutDiv from '../LayoutDiv.vue';\nimport Swal from 'sweetalert2';\nconst fs = require('fs');\nexport default {\n  name: 'ProjectCreate',\n  components: {\n    LayoutDiv\n  },\n  data() {\n    return {\n      showErr: false,\n      msgErr: [],\n      project: {\n        //   name: null,\n        //   email: null,\n        //   phone_number: null,\n        //   date_of_born: null,\n        //   place_of_born: null,\n        //   sex: null\n        name: 'Edo Aprilianto',\n        email: 'edoapriliano70@gmail.com',\n        phone_number: '081908802234',\n        date_of_born: '05/04/1998',\n        place_of_born: 'jakarta',\n        sex: 'Male'\n      },\n      sexs: ['Male', 'Female'],\n      isSaving: false\n    };\n  },\n  methods: {\n    save() {\n      const data = JSON.stringify(this.project);\n      fs.readFile('form.json', 'utf8', function (err, data) {\n        if (err) throw err;\n        console.log(data);\n      });\n      return false;\n      if (this.validation()) {\n        this.isSaving = true;\n        axios.post('/form', this.project).then(response => {\n          Swal.fire({\n            icon: 'success',\n            title: 'Project saved successfully!',\n            showConfirmButton: false,\n            timer: 1500\n          });\n          this.isSaving = false;\n          return response;\n        }).catch(error => {\n          console.log(error.response.data);\n          this.msgErr = error.response.data.message;\n          this.isSaving = false;\n          this.showErr = true;\n          Swal.fire({\n            icon: 'error',\n            title: 'An Error Occured!',\n            showConfirmButton: false,\n            timer: 1500\n          });\n          return error;\n        });\n      }\n    },\n    validation() {\n      console.log(this.project.phone_number);\n      if (this.project.name === null) {\n        Swal.fire({\n          icon: 'error',\n          title: 'Name is Required!',\n          showConfirmButton: false,\n          timer: 1500\n        });\n        return false;\n      }\n      if (this.project.email === null) {\n        Swal.fire({\n          icon: 'error',\n          title: 'Email is Required!',\n          showConfirmButton: false,\n          timer: 1500\n        });\n        return false;\n      }\n      if (this.project.phone_number === null) {\n        Swal.fire({\n          icon: 'error',\n          title: 'Phone Number is Required!',\n          showConfirmButton: false,\n          timer: 1500\n        });\n        return false;\n      }\n      if (this.project.date_of_born === null) {\n        Swal.fire({\n          icon: 'error',\n          title: 'Born of Date is Required!',\n          showConfirmButton: false,\n          timer: 1500\n        });\n        return false;\n      }\n      if (this.project.place_of_born === null) {\n        Swal.fire({\n          icon: 'error',\n          title: 'Place of Date is Required!',\n          showConfirmButton: false,\n          timer: 1500\n        });\n        return false;\n      }\n      if (this.project.sex === null) {\n        Swal.fire({\n          icon: 'error',\n          title: 'Sex is Required!',\n          showConfirmButton: false,\n          timer: 1500\n        });\n        return false;\n      }\n      return true;\n    }\n  }\n};","map":{"version":3,"names":["axios","LayoutDiv","Swal","fs","require","name","components","data","showErr","msgErr","project","email","phone_number","date_of_born","place_of_born","sex","sexs","isSaving","methods","save","JSON","stringify","readFile","err","console","log","validation","post","then","response","fire","icon","title","showConfirmButton","timer","catch","error","message"],"sources":["/Users/pinjamyuk/Project/vue-assesment/src/components/pages/FormCreate.vue"],"sourcesContent":["<template>\n    <layout-div>\n      <h2 class=\"text-center mt-5 mb-3\">Assesment Form Input</h2>\n      <div class=\"card\">\n\n          \n          <div class=\"card-body\">\n            <ul id=\"error\" v-if=\"showErr\">\n                <li v-for=\"row in msgErr\" :value=\"row.msg\" :key=\"row\">\n                 <span class=\"text-danger\">{{row.msg}}</span>\n                </li>\n            </ul>\n              <form>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"name\">Name</label>\n                      <input \n                          v-model=\"project.name\"\n                          type=\"text\"\n                          class=\"form-control\"\n                          id=\"name\"\n                          :require=true\n                          name=\"name\"/>\n                  </div>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"name\">Email</label>\n                      <input \n                          v-model=\"project.email\"\n                          type=\"text\"\n                          class=\"form-control\"\n                          id=\"email\"\n                          :require=true\n                          name=\"email\"/>\n                  </div>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"name\">Phone Number</label>\n                      <input \n                          v-model=\"project.phone_number\"\n                          type=\"text\"\n                          class=\"form-control\"\n                          id=\"number\"\n                          :require=true\n                          name=\"number\"/>\n                  </div>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"bod\">Born of Date</label>\n                      <input \n                          v-model=\"project.date_of_born\"\n                          type=\"date\"\n                          class=\"form-control\"\n                          id=\"bod\"\n                          :require=true\n                          name=\"bod\"/>\n                  </div>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"pod\">Place of Date</label>\n                      <input \n                          v-model=\"project.place_of_born\"\n                          type=\"text\"\n                          class=\"form-control\"\n                          id=\"pod\"\n                          :require=true\n                          name=\"pod\"/>\n                  </div>\n                  <div class=\"form-group\">\n                      <label htmlFor=\"pod\">Sex</label>\n                      <select class=\"form-control\" v-model=\"project.sex\" :require=\"true\">\n                        <option  v-for=\"item in sexs\" :value=\"item\" :key=\"item\">{{ item }}</option>\n                    </select>\n                  </div>\n                  <button \n                      @click=\"save()\"\n                      :disabled=\"isSaving\"\n                      type=\"button\"\n                      class=\"btn btn-outline-primary mt-3\">\n                      Save\n                  </button>\n              </form>\n              <div>\n                <table class=\"table\">\n                </table>\n             </div>\n          </div>\n      </div>\n    </layout-div>\n  </template>\n   \n  <script>\n  import axios from 'axios';\n  import LayoutDiv from '../LayoutDiv.vue';\n  import Swal from 'sweetalert2'\n  const fs = require('fs');\n\n   \n  export default {\n    name: 'ProjectCreate',\n    components: {\n      LayoutDiv,\n    },\n    data() {\n      return {\n        showErr: false,\n        msgErr:[],\n        project: {\n        //   name: null,\n        //   email: null,\n        //   phone_number: null,\n        //   date_of_born: null,\n        //   place_of_born: null,\n        //   sex: null\n          name: 'Edo Aprilianto',\n          email: 'edoapriliano70@gmail.com',\n          phone_number: '081908802234',\n          date_of_born: '05/04/1998',\n          place_of_born: 'jakarta',\n          sex: 'Male'\n        },\n        sexs : ['Male' ,'Female'],\n        isSaving:false,\n      };\n    },\n    methods: {\n        save() {\n\n            const data = JSON.stringify(this.project)\n            fs.readFile('form.json', 'utf8', function(err, data) {\n            if (err) throw err;\n            console.log(data);\n            });\n\n\n            return false;\n\n          if(this.validation()){\n            this.isSaving = true\n            axios.post('/form', this.project)\n                .then(response => {\n                Swal.fire({\n                    icon: 'success',\n                    title: 'Project saved successfully!',\n                    showConfirmButton: false,\n                    timer: 1500\n                })\n                this.isSaving = false\n                return response\n                })\n                .catch(error => {\n                console.log(error.response.data)\n                this.msgErr = error.response.data.message\n                this.isSaving = false\n                this.showErr = true\n                Swal.fire({\n                    icon: 'error',\n                    title: 'An Error Occured!',\n                    showConfirmButton: false,\n                    timer: 1500\n                })\n                return error\n                });\n          }\n      },\n      validation(){\n        console.log(this.project.phone_number)\n        if(this.project.name === null){\n            Swal.fire({\n                  icon: 'error',\n                  title: 'Name is Required!',\n                  showConfirmButton: false,\n                  timer: 1500\n              })\n              return false;\n        }\n        if(this.project.email === null){\n            Swal.fire({\n                  icon: 'error',\n                  title: 'Email is Required!',\n                  showConfirmButton: false,\n                  timer: 1500\n              })\n              return false;\n\n        }\n        if(this.project.phone_number === null){\n            Swal.fire({\n                  icon: 'error',\n                  title: 'Phone Number is Required!',\n                  showConfirmButton: false,\n                  timer: 1500\n              })\n              return false;\n\n        }\n        if(this.project.date_of_born === null){\n            Swal.fire({\n                  icon: 'error',\n                  title: 'Born of Date is Required!',\n                  showConfirmButton: false,\n                  timer: 1500\n              })\n              return false;\n\n        }\n        if(this.project.place_of_born === null){\n            Swal.fire({\n                  icon: 'error',\n                  title: 'Place of Date is Required!',\n                  showConfirmButton: false,\n                  timer: 1500\n              })\n              return false;\n\n        }\n        if(this.project.sex === null){\n            Swal.fire({\n                  icon: 'error',\n                  title: 'Sex is Required!',\n                  showConfirmButton: false,\n                  timer: 1500\n              })\n              return false;\n\n        }\n        return true\n      }\n    },\n  };\n  </script>"],"mappings":"AAuFE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,kBAAkB;AACxC,OAAOC,IAAG,MAAO,aAAY;AAC7B,MAAMC,EAAC,GAAIC,OAAO,CAAC,IAAI,CAAC;AAGxB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAC,EAAE;MACTC,OAAO,EAAE;QACT;QACA;QACA;QACA;QACA;QACA;QACEL,IAAI,EAAE,gBAAgB;QACtBM,KAAK,EAAE,0BAA0B;QACjCC,YAAY,EAAE,cAAc;QAC5BC,YAAY,EAAE,YAAY;QAC1BC,aAAa,EAAE,SAAS;QACxBC,GAAG,EAAE;MACP,CAAC;MACDC,IAAG,EAAI,CAAC,MAAK,EAAG,QAAQ,CAAC;MACzBC,QAAQ,EAAC;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACLC,IAAIA,CAAA,EAAG;MAEH,MAAMZ,IAAG,GAAIa,IAAI,CAACC,SAAS,CAAC,IAAI,CAACX,OAAO;MACxCP,EAAE,CAACmB,QAAQ,CAAC,WAAW,EAAE,MAAM,EAAE,UAASC,GAAG,EAAEhB,IAAI,EAAE;QA